<article>

    {{#if id}}
        <h2>{{title}}</h2>
        <h2>{{content}} uÅ¼ytkowniku o id {{id}}</h2>
    {{else}}
        <h2>Users</h2>

        {{#each users}}
            <div class="user">
                <h3>{{name}}</h3>
                <section>
                <h4>Address:</h4>
                <p>Street: {{address.street}}</p>
                <p>Suite: {{address.suite}}</p>
                <p>City: {{address.city}}</p>
                <p>Zipcode: {{address.zipcode}}</p>
                </section>
                <a href="">{{website}}</a>
            </div>
        {{/each}}

    {{/if}}
</article>